{
  "name": "set-interval-async",
  "version": "3.0.0",
  "description": "Modern version of setInterval for promises and async functions available in Node.js and browsers.",
  "repository": {
    "type": "git",
    "url": "https://github.com/ealmansi/set-interval-async.git"
  },
  "homepage": "https://github.com/ealmansi/set-interval-async",
  "keywords": [
    "setInterval",
    "setTimeout",
    "clearInterval",
    "clearTimeout",
    "interval",
    "timeout",
    "milliseconds",
    "seconds",
    "promise",
    "promises",
    "async",
    "asynchronous",
    "await",
    "recurrent",
    "concurrency",
    "reentrancy"
  ],
  "main": "./dist/dynamic/set-interval-async.cjs",
  "exports": {
    ".": {
      "require": "./dist/dynamic/set-interval-async.cjs",
      "import": "./dist/dynamic/set-interval-async.mjs"
    },
    "./dynamic": {
      "require": "./dist/dynamic/set-interval-async.cjs",
      "import": "./dist/dynamic/set-interval-async.mjs"
    },
    "./fixed": {
      "require": "./dist/fixed/set-interval-async.cjs",
      "import": "./dist/fixed/set-interval-async.mjs"
    }
  },
  "types": "./dist/dynamic/set-interval-async.d.cts",
  "unpkg": "./dist/browser/set-interval-async.iife.js",
  "jsdelivr": "./dist/browser/set-interval-async.iife.js",
  "library": "SetIntervalAsync",
  "files": [
    "./dist/browser/set-interval-async.iife.js",
    "./dist/dynamic/set-interval-async.cjs",
    "./dist/dynamic/set-interval-async.d.cts",
    "./dist/dynamic/set-interval-async.mjs",
    "./dist/fixed/set-interval-async.cjs",
    "./dist/fixed/set-interval-async.mjs"
  ],
  "author": "Emilio Almansi",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn build:tsc && yarn build:webpack",
    "build:tsc": "tsc",
    "build:webpack": "webpack",
    "lint": "eslint src test",
    "format": "prettier --write src test",
    "test": "nyc mocha",
    "coverage": "nyc report --reporter=lcov"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@sinonjs/fake-timers": "^9.1.2",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.6.4",
    "@types/sinon": "^10.0.13",
    "@types/sinonjs__fake-timers": "^8.1.2",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "babel-loader": "^8.2.5",
    "coveralls": "^3.1.1",
    "eslint": "^8.21.0",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "sinon": "^14.0.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
