    "promises",
    "async",
    "asynchronous",
    "await",
    "recurrent",
    "concurrency",
    "reentrancy"
  ],
  "main": "dist/set-interval-async.cjs.js",
  "module": "dist/set-interval-async.esm.mjs",
  "unpkg": "dist/set-interval-async.iife.js",
  "library": "SetIntervalAsync",
  "files": [
    "dynamic/**",
    "fixed/**",
    "legacy/**",
    "dist/**"
